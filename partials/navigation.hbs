<ul>
	{{#foreach navigation}}
		<li class="nav-{{slug}}{{#if current}} active{{/if}}"><a href="{{url absolute="true"}}"><span>{{label}}</span></a></li>
	{{/foreach}}
	{{#if @member}}
			<li><a class="account-link" href="/account/">Account</a></li>
			<li><a class="signout-link " href="#signout" data-members-signout>Log out</a></li>
	{{else}}
			<li><a class="signin-link" href="/signin/">Log in</a></li>
			<li><a class="signup-link" href="/signup/">Subscribe</a></li>
	{{/if}}
</ul>
